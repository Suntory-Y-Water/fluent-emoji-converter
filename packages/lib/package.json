{
  "name": "fluent-emoji-converter",
  "version": "0.0.1",
  "type": "module",
  "description": "Converts emoji to Fluent Emoji URLs",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:cov": "vitest run --coverage",
    "dev": "tsc --watch",
    "prepublishOnly": "pnpm build"
  },
  "keywords": [
    "emoji",
    "fluent",
    "converter",
    "microsoft",
    "typescript"
  ],
  "author": "suntory-n-water",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/suntory-n-water/fluent-emoji-converter.git"
  },
  "dependencies": {
    "@fluent-emoji-converter/core": "workspace:*"
  },
  "devDependencies": {
    "@fluent-emoji-converter/types": "workspace:*",
    "typescript": "^5.8.3",
    "vitest": "^3.2.4",
    "@vitest/coverage-v8": "^3.2.4"
  }
}
