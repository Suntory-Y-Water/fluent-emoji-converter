{
  "name": "fluent-emoji-converter-monorepo",
  "version": "0.0.1",
  "type": "module",
  "private": true,
  "description": "Monorepo for Fluent Emoji Converter",
  "scripts": {
    "build": "pnpm --filter \"./packages/*\" build",
    "test": "pnpm --filter \"./packages/*\" test",
    "test:cov": "pnpm --filter \"./packages/*\" test:cov",
    "typecheck": "pnpm --filter \"./packages/*\" typecheck",
    "lint": "pnpm --filter \"./packages/*\" lint",
    "dev": "pnpm --filter \"./packages/*\" dev"
  },
  "keywords": [
    "emoji",
    "fluent",
    "converter",
    "typescript"
  ],
  "author": "suntory-n-water",
  "license": "MIT",
  "devDependencies": {
    "@eslint/js": "^9.30.0",
    "@types/node": "^24.0.7",
    "@vitest/coverage-v8": "^3.2.4",
    "esbuild": "^0.19.0",
    "eslint": "^9.30.0",
    "prettier": "^3.6.2",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.35.0",
    "vitest": "^3.2.4"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "*"
      ]
    }
  }
}
